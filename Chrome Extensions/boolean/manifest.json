{
    "name": "Tracker",
    "description": "Will automatically jump to the recipe to avoid reading the blogs",
    "version": "1.0",
    "manifest_version": 3,
    
  "permissions": [
    "activeTab"
  ],
    "action":{ 
        "default_popup": "index.html"},
        "background": {
            "service_worker": "background.js"
          }
}